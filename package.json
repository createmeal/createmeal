{
  "name": "createmeal",
  "version": "1.3.1",
  "description": "A library to render HTML",
  "main": "dist/createmeal.js",
  "scripts": {
    "eslint": "eslint ./src/**/*",
    "build": "webpack",
    "build-tsc": "tsc --build",
    "docs": "node ./documentation/readme-generator.js",
    "test": "mocha",
    "release": "node release.js",
    "release_patch": "node release.js patch",
    "release_minor": "node release.js minor",
    "release_major": "node release.js major",
    "git_tag": "git tag -a $(echo $(npm pkg get version) | sed -e 's/\"//g') -m 'release new version'"
  },
  "author": "Julio Cezar da Silva (https://github.com/jcezardasilva)",
  "contributors": [
    "Marcelo Cesar da Silva (https://github.com/marcelocesards)"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/createmeal/createmeal.git"
  },
  "bugs": {
    "url": "https://github.com/createmeal/createmeal/issues"
  },
  "homepage": "https://createmeal.org/",
  "keywords": [
    "html",
    "json",
    "web",
    "frontend",
    "template"
  ],
  "license": "MIT",
  "devDependencies": {
    "chai": "^4.3.4",
    "eslint": "^8.7.0",
    "mocha": "^9.1.4",
    "node-fetch": "^3.1.1",
    "webpack": "^5.58.0",
    "webpack-cli": "^4.9.0"
  },
  "directories": {
    "test": "test"
  },
  "type": "module"
